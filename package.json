{
  "name": "pacman",
  "version": "1.1.0",
  "description": "School project, a multiplayer pac-man kind of game.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-dev": "babel ./server/src/index.js --watch --out-file ./server/dist/index.js",
    "build": "babel ./server/src/index.js --out-file ./server/dist/index.js",
    "build-client-babel": "babel ./client/js/src/start.js --out-file ./client/js/dist/babel/start.js && babel ./client/js/src/game.js --out-file ./client/js/dist/babel/game.js",
    "build-start": "browserify ./client/js/dist/babel/start.js -o ./client/js/dist/browserified/start.js",
    "build-game": "browserify ./client/js/dist/babel/game.js -o ./client/js/dist/browserified/game.js",
    "start-server": "npm run build-client-babel && npm run build-start && npm run build-game && node ./server/dist/index.js"
  },
  "keywords": [
    "pacman"
  ],
  "author": "5Zei",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "browserify": "^13.1.1"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "express": "^4.14.0",
    "http": "0.0.0",
    "jquery": "^3.1.1",
    "path": "^0.12.7",
    "toastr": "^2.1.2",
    "websocket": "^1.0.23"
  }
}
